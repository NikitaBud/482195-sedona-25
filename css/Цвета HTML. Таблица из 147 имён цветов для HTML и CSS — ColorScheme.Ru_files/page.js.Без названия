$(function(){if(1500<$(document).height()){var o=$(".scroll_up"),l=$(".scroll_down");300<$(window).scrollTop()&&o.show(),$(window).scrollTop()+1200<$(document).height()&&l.show(),$(window).scroll(function(){300<$(this).scrollTop()?o.fadeIn(200):o.fadeOut(200),$(this).scrollTop()+1200>$(document).height()?l.fadeOut(200):l.fadeIn(200)}),o.click(function(){return $("html, body").animate({scrollTop:0},300),!1}),l.click(function(){return $("html, body").animate({scrollTop:$(document).height()},300),!1})}});